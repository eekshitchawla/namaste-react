{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://swiggy-clone-rho.vercel.app/",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
